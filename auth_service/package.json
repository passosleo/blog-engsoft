{
  "name": "spends-api",
  "description": "API for Spends app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "author": "Leonardo Passos",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/src/app/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "prebuild": "rimraf ./dist",
    "build": "yarn prebuild && tsc && ts-node build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "logs:clear": "rimraf ./logs",
    "test": "jest --passWithNoTests",
    "prisma:seed": "npx prisma db seed",
    "prisma:push": "npx prisma db push",
    "prisma:update": "npx prisma generate && npx prisma db push",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:dev": "npx prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.1.0",
    "@types/nodemailer": "^6.4.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-unused-imports": "^3.1.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.9.1",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/passosleo/spends-api.git"
  }
}
